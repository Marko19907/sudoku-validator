{
  "name": "sudoku-validator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "buildCI": "CI=false && craco build",
    "test": "craco test",
    "analyze": "yarn cra-bundle-analyzer -r ../report.html && start report.html"
  },
  "homepage": "https://marko19907.github.io/sudoku-validator",
  "eslintConfig": {
    "rules": {
      "semi": "error",
      "@typescript-eslint/array-type": [
        "error",
        {
          "default": "generic"
        }
      ]
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "eslint": "^8.8.0",
    "lodash": "npm:lodash-es",
    "preact": "^10.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "use-file-picker": "^1.4.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/lodash": "npm:@types/lodash-es",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "cra-bundle-analyzer": "^0.1.1",
    "typescript": "^4.7.4"
  }
}
